{
    "name": "bottemplate",
    "packageManager": "yarn@3.2.1",
    "author": {
        "name": "Florian Reichelt | Fllooo",
        "email": "mail@florian-reichelt.de",
        "url": "https://florian-reichelt.de"
    },
    "version": "1.0.0",
    "scripts": {
        "dev": "yarn cross-env NODE_ENV=development nodemon --watch src/ --watch package.json --watch data/ -L --inspect=0.0.0.0:9229",
        "docker-dev": "docker-compose -f docker-compose-dev.yml up --build -d",
        "register-cmds": "yarn ts-node scripts/registerCommands.ts",
        "register-cmds:dev": "yarn cross-env NODE_ENV=development yarn ts-node scripts/registerCommands.ts",
        "prettier-format": "yarn run-script-os",
        "prettier-format:win32": "prettier --config .prettierrc.js \"./src/**/*.ts\" --write",
        "prettier-format:darwin:linux": "prettier --config .prettierrc.js 'src/**/*.ts' --write",
        "prettier-format:default": "prettier --config .prettierrc.js 'src/**/*.ts' --write",
        "lint": "yarn run eslint --fix ./src/"
    },
    "dependencies": {
        "@discordjs/builders": "^0.13.0",
        "@discordjs/rest": "^0.4.1",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^17.0.36",
        "@types/util.promisify": "^1.0.4",
        "cli-color": "^2.0.2",
        "discord-api-types": "^0.33.0",
        "discord-giveaways": "^5.1.1",
        "discord.js": "^13.7.0",
        "fs": "^0.0.1-security",
        "i18next": "^21.8.5",
        "i18next-localstorage-cache": "^1.1.1",
        "i18next-node-fs-backend": "^2.1.3",
        "i18next-node-mongodb-backend": "^0.0.6",
        "mongoose": "^6.3.4",
        "ms": "^2.1.3",
        "node-dig-dns": "^0.3.2",
        "node-notifier": "^10.0.1",
        "parse-ms": "^3.0.0"
    },
    "devDependencies": {
        "@swc/core": "^1.2.194",
        "@swc/core-win32-x64-msvc": "^1.2.194",
        "@swc/helpers": "^0.3.16",
        "@types/eslint": "^8.4.2",
        "@typescript-eslint/eslint-plugin": "^5.26.0",
        "@typescript-eslint/parser": "^5.26.0",
        "@yarnpkg/sdks": "^3.0.0-rc.6",
        "cross-env": "^7.0.3",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "nodemon": "^2.0.16",
        "onchange": "^7.1.0",
        "prettier": "^2.6.2",
        "regenerator-runtime": "^0.13.9",
        "run-script-os": "^1.1.6",
        "ts-loader": "^9.3.0",
        "ts-node": "^10.8.0",
        "ts-node-dev": "^2.0.0",
        "tsc": "^2.0.4",
        "typescript": "^4.7.2"
    }
}
